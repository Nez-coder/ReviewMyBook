<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reviewmybook">
    <uses-permission android:name = "android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/images_icon"
        android:label="@string/app_name"

        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.activityhome">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"/>
            <action android:name="android.intent.action.MAIN" />


            </intent-filter>
        </activity>
        <activity android:name=".activity.activitymainreview"></activity>
        <activity android:name=".activity.activityreviews"></activity>
        <activity android:name=".activity.activityresults"></activity>

    </application>

</manifest>